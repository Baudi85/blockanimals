{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "tntgames:tnt",
      "materials": { "default": "blocklingbase" },
      "textures": {
        "default": "textures/entity/block_animals/blockling_tnt",
        "exploding": "textures/entity/block_animals/blockling_tnt_exploding"
      },
      "geometry": {
        "default": "geometry.blocklingbase"
      },
      "animations": {
        "move": "animation.blocklingbase.move",
        "swelling": "animation.blockling_tnt.swelling"
      },
      "scripts": {
        "animate": [
          { "swelling": "query.is_powered" },
          { "move": "query.modified_move_speed" }
        ]
      },
      "sound_effects": {
        "fuse": "blockling_tnt.fuse"
      },
      "render_controllers": [ "controller.render.blockling_tnt" ],
      "spawn_egg": {
        "texture": "blockling_spawn_egg",
        "texture_index": 52
      }
    }
  }
}
